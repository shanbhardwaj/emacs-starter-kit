(custom-set-variables
  ;; custom-set-variables was added by Custom.
  ;; If you edit it by hand, you could mess it up, so be careful.
  ;; Your init file should contain only one such instance.
  ;; If there is more than one, they won't work right.
 '(abbrev-file-name "~/.emacs.d/cache/Abbreviations")
 '(auto-save-list-file-prefix "~/.emacs.d/auto-save-list/.saves-")
 '(blink-cursor-mode t)
 '(browse-url-browser-function (quote browse-url-default-macosx-browser))
 '(column-number-mode t)
 '(desktop-path (quote ("~/.emacs.d/cache/" "." "~")))
 '(display-battery-mode t)
 '(display-time-mode t)
 '(ecb-basic-buffer-sync nil)
 '(ecb-cache-directory-contents-not nil)
 '(ecb-layout-window-sizes (quote (("left14" (0.1827956989247312 . 0.7) (0.1827956989247312 . 0.2833333333333333)))))
 '(ecb-options-version "2.40")
 '(ecb-vc-enable-support t)
 '(ecb-version-check nil)
 '(initial-major-mode (quote org-mode))
 '(longlines-auto-wrap t)
 '(org-agenda-files (quote ("~/org/gigzee.org" "~/org/unscrewed.org" "~/org/todo.org" "~/org/refile.org" "~/org/journal.org" "~/org/zoomcourt.org")))
 '(recentf-save-file "~/.emacs.d/cache/Recent Files.el")
 '(resize-mini-windows nil t)
 '(savehist-file "~/.emacs.d/cache/minibuffer-history.el")
 '(show-paren-mode t)
 '(size-indication-mode t)
 '(visual-line-mode t t)
 '(word-wrap t))
(custom-set-faces
  ;; custom-set-faces was added by Custom.
  ;; If you edit it by hand, you could mess it up, so be careful.
  ;; Your init file should contain only one such instance.
  ;; If there is more than one, they won't work right.
 '(ecb-default-general-face ((((class color) (background light)) (:height 100))))
 '(mode-line ((((class color) (min-colors 88)) (:background "purple1" :foreground "black" :box (:line-width -1 :style released-button))))))
